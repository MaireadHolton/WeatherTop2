<table class="ui table">
    <tbody>
    {{#each station.readings}}
      <thead>
      <tr>
        <td>
          {{code}}
        </td>
        <td>
          {{temp}}
        </td>
        <td>
          {{windSpeed}}
        </td>
        <td>
          {{windDirection}}
        </td>
        <td>
          {{pressure}}
        </td>
        <td>
          <a href="/station/{{../station.id}}/deletereading/{{id}}" class="ui red icon button">
            <i class="icon trash"></i>
        </td>
      </tr>
    </thead>
   {{/each}}
      <tr>
  <th> Code </th>
  <th> Temp </th>
  <th> Wind Speed </th>
  <th> Wind Direction </th>
  <th> Pressure </th>
</tr>
  </tbody>
</table>