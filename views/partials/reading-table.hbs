<table class="ui table">
    <tbody>
    {{#each station.readings}}
      <tr>
        <td>
          {{date}}
        </td>
        <td>
          {{code}}
        </td>
        <td>
          {{temp}}
        </td>
        <td>
          {{windspeed}}
        </td>
        <td>
          {{winddirection}}
        </td>
        <td>
          {{pressure}}
        </td>
        <td>
          <a href="/station/{{../station.id}}/deletereading/{{id}}" class="ui red icon button">
            <i class="icon trash"></i>
        </td>
      </tr>
   {{/each}}
      <tr>
  <th> Date/Time </th>
  <th> Code </th>
  <th> Temp </th>
  <th> Wind Speed </th>
  <th> Wind Direction </th>
  <th> Pressure </th>
</tr>
  </tbody>
</table>